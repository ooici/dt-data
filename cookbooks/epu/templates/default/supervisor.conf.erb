<% @epuservice_list.each do |epuservice| %>
[program:<%= epuservice[:program] %>]
command=<%= epuservice[:command] %>
autorestart=<%= epuservice[:autorestart] %>
stdout_logfile=%(here)s/logs/%(program_name)s_stdout.log
stderr_logfile=%(here)s/logs/%(program_name)s_stderr.log
<% end %>

[unix_http_server]
file=%(here)s/<%= @epuservice_name %>-supervisor.sock

[supervisord]
logfile=%(here)s/logs/<%= @epuservice_name %>-supervisord.log
pidfile=%(here)s/<%= @epuservice_name %>-supervisord.pid

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix://%(here)s/<%= @epuservice_name %>-supervisor.sock
